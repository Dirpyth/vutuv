</header>
<%= render Vutuv.LayoutView, "flash.html", assigns %>

<div class="breadcrumbs">
  <%= gen_breadcrumbs([
    gettext("Users"),
    {full_name(@user), user_path(@conn, :show, @user)},
    {gettext("Addresses"), user_address_path(@conn, :index, @user)},
    gettext("New")]) %>
</div>


<div class="card-list">

  <aside>
    <%= render Vutuv.SharedView, "_ad.html", assigns %>
  </aside>
  <section class="card">

  <%= render "form_"<>@country<>".html", backlink: user_address_path(@conn, :index, @user), changeset: @changeset, conn: @conn, action: user_address_path(@conn, :create, @user) %>
  </section>
</div>
